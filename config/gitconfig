[user]
	name = James Ottaway
	email = git@james.ottaway.io
[color]
	ui = true
[github]
	user = jamesottaway
[help]
	autocorrect = 1
[core]
	pager = 
	autocrlf = input
[apply]
	whitespace = warn
[push]
	default = current
[pager]
	diff = diff-highlight
[alias]
	a = add
	ai = add --interactive
	aa = add --all
	b = branch
	c = commit
	cm = commit --message
	cl = clone
	co = checkout
	conf = config
	d = diff
	dc = diff --cached
	discard = checkout --
	l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset' --abbrev-commit
	latest = log -n 1
	pom = push origin master
	pod = push origin develop
	phm = push heroku master
	rs = reset --soft
	rh = reset --hard
	s = status --short
	unstage = reset HEAD
	who = shortlog -s --
	tfc = tf checkin
	tfp = tf pull
	tfpr = tf pull --rebase
